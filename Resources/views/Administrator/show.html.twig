{% extends 'BoShurikAdminBundle::layout.html.twig' %}

{% block content %}
<h1>Administrator</h1>

<ul>
    <li>
        <a href="{{ path('admin_administrator') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_administrator_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
</ul>

<table class="table">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Username</th>
            <td>{{ entity.username }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>
        <tr>
            <th>Enabled</th>
            <td>{{ entity.enabled }}</td>
        </tr>
        <tr>
            <th>Expired</th>
            <td>{{ entity.expired }}</td>
        </tr>
        <tr>
            <th>Locked</th>
            <td>{{ entity.locked }}</td>
        </tr>
        <tr>
            <th>Lastlogin</th>
            <td>{{ entity.lastLogin|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Lastip</th>
            <td>{{ entity.lastIp }}</td>
        </tr>
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('admin_administrator') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_administrator_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
</ul>
{% endblock %}